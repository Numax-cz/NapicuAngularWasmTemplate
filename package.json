{
  "name": "napicu-angular-wasm-template",
  "version": "1.0.0",
  "description": "A collection of components and services for Angular 2+ to demonstrate integration with WebAssembly",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "npm run wasm && ng build --configuration production",
    "wasm": "node scripts/compile-wasm.js && node scripts/wasm-cache-busting.js",
    "start": "ng serve",
    "lint": "ng lint",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --check . --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boyanio/angular-wasm.git"
  },
  "keywords": [
    "Angular",
    "WebAssembly",
    "JavaScript"
  ],
  "private": false,
  "author": "Boyan Mihaylov",
  "dependencies": {
    "@angular/common": "^15.2.7",
    "@angular/compiler": "^15.2.7",
    "@angular/core": "^15.2.7",
    "@angular/forms": "^15.2.7",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "^15.2.7",
    "@angular/router": "^15.2.7",
    "chalk": "^4.1.2",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.7",
    "@types/node": "~18.15.11",
    "codelyzer": "^6.0.2",
    "tslint": "~6.1.0",
    "typescript": "4.9.5"
  }
}
