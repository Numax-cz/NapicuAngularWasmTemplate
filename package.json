{
  "name": "angular-wasm",
  "version": "0.1.0",
  "description": "A collection of components and services for Angular 2+ to demonstrate integration with WebAssembly",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "npm run wasm && ng build --prod",
    "wasm": "node scripts/compile-wasm.js && node scripts/wasm-cache-busting.js",
    "start": "ng serve",
    "lint": "ng lint",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --check . --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boyanio/angular-wasm.git"
  },
  "keywords": [
    "Angular",
    "WebAssembly",
    "JavaScript"
  ],
  "private": false,
  "author": "Boyan Mihaylov",
  "dependencies": {
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/forms": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "angular2-ladda": "^3.0.0",
    "ladda": "^2.0.1",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.1",
    "@angular/cli": "^11.0.1",
    "@angular/compiler-cli": "^11.0.0",
    "@types/node": "~10.12.18",
    "chalk": "^4.0.0",
    "codelyzer": "^6.0.0",
    "prettier": "^2.0.5",
    "tslint": "~6.1.0",
    "typescript": "^4.0.2"
  }
}
